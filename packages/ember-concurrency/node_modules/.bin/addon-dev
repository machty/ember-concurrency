#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules/@embroider/addon-dev/src/node_modules:/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules/@embroider/addon-dev/node_modules:/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules/@embroider/node_modules:/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules:/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules/@embroider/addon-dev/src/node_modules:/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules/@embroider/addon-dev/node_modules:/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules/@embroider/node_modules:/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules:/Users/machty/code/ember/ember-concurrency/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules/@embroider/addon-dev/src/commands.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@embroider+addon-dev@4.2.0_rollup@4.9.6/node_modules/@embroider/addon-dev/src/commands.js" "$@"
fi
