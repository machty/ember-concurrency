{
  "name": "ember-cli-eslint",
  "version": "5.1.0",
  "description": "Ember-cli eslint support, for checking your application matches your coding standards.",
  "keywords": [
    "ember",
    "ember-addon",
    "eslint",
    "eslint ember",
    "eslint ember-cli",
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-cli/ember-cli-eslint.git"
  },
  "license": "MIT",
  "author": "Jonathan Kingston",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://github.com/ember-cli/ember-cli-eslint",
  "bugs": {
    "url": "https://github.com/ember-cli/ember-cli-eslint/issues"
  },
  "files": [
    "blueprints",
    "index.js"
  ],
  "main": "index.js",
  "scripts": {
    "build": "ember build",
    "changelog": "lerna-changelog",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "mocha node-tests --recursive",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "broccoli-lint-eslint": "^5.0.0",
    "ember-cli-version-checker": "^3.0.0",
    "rsvp": "^4.6.1",
    "walk-sync": "^1.0.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.0.2",
    "ember-cli": "~3.7.1",
    "ember-cli-babel": "^7.1.2",
    "ember-cli-blueprint-test-helpers": "^0.19.1",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.0.0",
    "ember-cli-inject-live-reload": "^2.0.0",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.7.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.0.1",
    "eslint-plugin-node": "^8.0.0",
    "express": "^4.12.3",
    "fs-extra": "^7.0.0",
    "lerna-changelog": "^0.8.2",
    "loader.js": "^4.7.0",
    "mocha": "^5.0.0",
    "qunit-dom": "^0.8.0",
    "testdouble": "^3.9.3"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": "ember-cli-qunit",
    "main": "index.js",
    "defaultBlueprint": "ember-cli-eslint"
  }
}
