{
  "name": "ember-cli-htmlbars",
  "version": "3.1.0",
  "description": "A library for adding htmlbars to ember CLI",
  "keywords": [
    "ember-addon",
    "ember-cli"
  ],
  "homepage": "https://github.com/ember-cli/ember-cli-htmlbars",
  "bugs": {
    "url": "https://github.com/ember-cli/ember-cli-htmlbars/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ember-cli/ember-cli-htmlbars.git"
  },
  "license": "MIT",
  "author": "Jonathan Jackson & Chase McCarthy",
  "files": [
    "index.js",
    "addDependencyTracker.js",
    "ember-addon-main.js",
    "utils.js"
  ],
  "main": "index.js",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "yarn run test:ember && yarn run test:node",
    "test:ember": "ember test",
    "test:node": "mocha node-tests/*.js",
    "test:node:debug": "mocha debug node-tests/*.js"
  },
  "dependencies": {
    "broccoli-persistent-filter": "^2.3.1",
    "hash-for-dep": "^1.5.1",
    "json-stable-stringify": "^1.0.1",
    "strip-bom": "^3.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-test-helper": "^2.0.0",
    "chai": "^4.2.0",
    "co": "^4.6.0",
    "ember-cli": "~3.9.0",
    "ember-cli-app-version": "^3.2.0",
    "ember-cli-babel": "^7.8.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-template-lint": "^1.0.0-beta.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.1.3",
    "ember-source": "~3.11.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.2.1",
    "eslint-plugin-ember": "^6.7.0",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-node": "^8.0.0",
    "fixturify": "^1.2.0",
    "loader.js": "^4.7.0",
    "mocha": "^6.1.4",
    "qunit-dom": "^0.8.5"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "main": "ember-addon-main.js",
    "configPath": "tests/dummy/config"
  }
}
